{
  "name": "react-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/react-client/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "options": {
      "command": ""
    },
    "docker-build": {
      "executor": "nx:run-commands",
      "configurations": {
        "staging": {
          "command": "docker build -f apps/react-client/deploy/docker/Dockerfile.staging . -t react-client-staging:${IMAGE_VERSION:-latest}"
        },
        "production": {
          "command": "docker build -f ${DOCKERFILE_PATH:-apps/react-client/deploy/docker/Dockerfile.production} . -t react-client-production:${IMAGE_VERSION:-latest}"
        }
      }
    }
  }
}
